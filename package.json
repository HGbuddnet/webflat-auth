{
  "name": "@hgbuddnet/webflat-auth",
  "version": "1.0.1",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:js": "tsup",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "yarn clean && yarn build:js && yarn build:types",
    "dev:js": "tsup --watch",
    "dev:types": "tsc -p tsconfig.build.json -w",
    "dev": "yarn dev:js & yarn dev:types",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "@tanstack/react-query": ">=5.90.0",
    "@hgbuddnet/webflat-service": ">=1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tanstack/react-query": "^5.90.7",
    "@hgbuddnet/webflat-service": "git+ssh://git@github.com/hgbuddnet/webflat-service.git#v1.0.0"
  }
}
